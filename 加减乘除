# include <stdio.h>
# include <string.h>
int main()
{
	char strExp[]="2+2*3+2/2-1";
    int res=strExp[0]-'0';
	for(int i=1;i<strlen(strExp);i++)
	{   
			if(strExp[i]=='*')
			{    
				strExp[i-1]=((strExp[i-1]-'0')*(strExp[i+1]-'0'))+'0';
				i++;
			}
            else if(strExp[i]=='/')
			{
                strExp[i-1]=((strExp[i-1]-'0')/(strExp[i+1]-'0'))+'0';
				i++;
			}
	} 
	for(int j=0;j<strlen(strExp);j++)
	{
		if(strExp[j]=='+')
		{
			int b=strExp[j+1]-'0';
			res=res+b;
			j++;
		}
		else if(strExp[j]=='-')
		{
			int c=strExp[j+1]-'0';
			res=res-c;
		j++;
		}
	}
	printf("输出结果：%d\n",res);
	return 0;
}

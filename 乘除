# include <stdio.h>
# include <string.h>
int main()
{
	char strExp[]="2*2/4*1/1*2*3/2";
    int	res=strExp[0]-'0';
	for(int i=1;i<strlen(strExp);i++)
	{
		if(strExp[i]=='*')
		{
			int right=strExp[i+1]-'0';
			res=res*right;
			i++;
		}
		else if(strExp[i]=='/')
		{
			int left=strExp[i+1]-'0';
			res=res/left;
			i++;
		}
	}
	
	printf("%d\n",res);
	return 0;
}
